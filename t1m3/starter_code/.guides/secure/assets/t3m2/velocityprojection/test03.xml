<scene>
  <simtype type="rigid-body"/>
  <description text="A tumbling line of dominos. Since we have not implemented a friction model, the mass of the bottoms of the dominos has been increased so that they stay more or less in place. The whole line of dominos should tumble."/>
  <viewport cx="0.0" cy="0.0" size="10.0"/>
  <maxsimfreq max="100000000.0"/>
  <duration time="5.0"/>

  <rigidbodyintegrator type="symplectic-euler" dt="0.001"/>

  <rigidbodycollisionhandling detection="all-pairs" response="velocity-projection"/>

  <!-- Rigid body serving as a 'table' -->
  <rigidbodyvertex x="-12.0" y="-5.0" m="1"/>
  <rigidbodyvertex x="-12.0" y="-3.0" m="1"/>
  <rigidbodyvertex x="12.0" y="-3.0" m="1"/>
  <rigidbodyvertex x="12.0" y="-5.0" m="1"/>
  <rigidbody p="0" p="1" p="2" p="3" vx="0.0" vy="0.0" omega="0.0" r="0.25" fixed="1"/>

  <rigidbodyvertex x="-8.0" y="-2.5" m="5"/>
  <rigidbodyvertex x="-7.5" y="-2.5" m="5"/>
  <rigidbodyvertex x="-7.5" y="2.5" m="1"/>
  <rigidbodyvertex x="-8.0" y="2.5" m="1"/>

  <rigidbody p="4" p="5" p="6" p="7" vx="0" vy="0" omega="-3" r="0.25" fixed="0"/>

  <rigidbodyvertex x="-6.0" y="-2.5" m="5"/>
  <rigidbodyvertex x="-5.5" y="-2.5" m="5"/>
  <rigidbodyvertex x="-5.5" y="2.5" m="1"/>
  <rigidbodyvertex x="-6.0" y="2.5" m="1"/>

  <rigidbody p="8" p="9" p="10" p="11" vx="0" vy="0" omega="0" r="0.25" fixed="0"/>

  <rigidbodyvertex x="-4.0" y="-2.5" m="5"/>
  <rigidbodyvertex x="-3.5" y="-2.5" m="5"/>
  <rigidbodyvertex x="-3.5" y="2.5" m="1"/>
  <rigidbodyvertex x="-4.0" y="2.5" m="1"/>

  <rigidbody p="12" p="13" p="14" p="15" vx="0" vy="0" omega="0" r="0.25" fixed="0"/>

  <rigidbodyvertex x="-2.0" y="-2.5" m="5"/>
  <rigidbodyvertex x="-1.5" y="-2.5" m="5"/>
  <rigidbodyvertex x="-1.5" y="2.5" m="1"/>
  <rigidbodyvertex x="-2.0" y="2.5" m="1"/>

  <rigidbody p="16" p="17" p="18" p="19" vx="0" vy="0" omega="0" r="0.25" fixed="0"/>

  <rigidbodyvertex x="0.0" y="-2.5" m="5"/>
  <rigidbodyvertex x="0.5" y="-2.5" m="5"/>
  <rigidbodyvertex x="0.5" y="2.5" m="1"/>
  <rigidbodyvertex x="0.0" y="2.5" m="1"/>

  <rigidbody p="20" p="21" p="22" p="23" vx="0" vy="0" omega="0" r="0.25" fixed="0"/>

  <rigidbodyvertex x="2.0" y="-2.5" m="5"/>
  <rigidbodyvertex x="2.5" y="-2.5" m="5"/>
  <rigidbodyvertex x="2.5" y="2.5" m="1"/>
  <rigidbodyvertex x="2.0" y="2.5" m="1"/>

  <rigidbody p="24" p="25" p="26" p="27" vx="0" vy="0" omega="0" r="0.25" fixed="0"/>

  <rigidbodyvertex x="4.0" y="-2.5" m="5"/>
  <rigidbodyvertex x="4.5" y="-2.5" m="5"/>
  <rigidbodyvertex x="4.5" y="2.5" m="1"/>
  <rigidbodyvertex x="4.0" y="2.5" m="1"/>

  <rigidbody p="28" p="29" p="30" p="31" vx="0" vy="0" omega="0" r="0.25" fixed="0"/>

  <rigidbodyvertex x="6.0" y="-2.5" m="5"/>
  <rigidbodyvertex x="6.5" y="-2.5" m="5"/>
  <rigidbodyvertex x="6.5" y="2.5" m="1"/>
  <rigidbodyvertex x="6.0" y="2.5" m="1"/>

  <rigidbody p="32" p="33" p="34" p="35" vx="0" vy="0" omega="0" r="0.25" fixed="0"/>

  <rigidbodyvertex x="8.0" y="-2.5" m="5"/>
  <rigidbodyvertex x="8.5" y="-2.5" m="5"/>
  <rigidbodyvertex x="8.5" y="2.5" m="1"/>
  <rigidbodyvertex x="8.0" y="2.5" m="1"/>

  <rigidbody p="36" p="37" p="38" p="39" vx="0" vy="0" omega="0" r="0.25" fixed="0"/>

  <rigidbodygravityforce fx="0.0" fy="-10.0"/>

</scene>
