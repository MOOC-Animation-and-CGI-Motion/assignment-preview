<scene>
  <simtype type="rigid-body"/>
  <description text="An airplane fixed in space to a spring subject to gravity and a wind force. The airplane should reach and equilibrium between the three forces."/>
  <duration time="50.0"/>
  <maxsimfreq max="500000.0"/>
  <viewport cx="-2.0" cy="0.0" size="16.0"/>

  <rigidbodyintegrator type="symplectic-euler" dt="0.001"/>

  <rigidbodyvertex x="-10" y="0" m="1"/>
  <rigidbodyvertex x="0" y="1" m="1"/>
  <rigidbodyvertex x="2" y="10" m="1"/>
  <rigidbodyvertex x="2" y="1" m="1"/>
  <rigidbodyvertex x="6" y="1" m="1"/>
  <rigidbodyvertex x="8" y="4" m="1"/>
  <rigidbodyvertex x="8" y="-4" m="1"/>
  <rigidbodyvertex x="6" y="-1" m="1"/>
  <rigidbodyvertex x="2" y="-1" m="1"/>
  <rigidbodyvertex x="2" y="-10" m="1"/>
  <rigidbodyvertex x="0" y="-1" m="1"/>
  <rigidbody p="0" p="1" p="2" p="3" p="4" p="5" p="6" p="7" p="8" p="9" p="10" vx="0.0" vy="0.0" omega="0.0" r="0.2"/>

  <rigidbodyspringforce  i="-1" pix="-15" piy="0.0" j="0" pjx="-12.0" pjy="0.0" k="40.0" l0="5.35"/>

  <rigidbodygravityforce fx="0.0" fy="-2.0"/>
  <rigidbodywindforce fx="4.0" fy="0.0" pointsperedge="4" beta="1.0"/>

</scene>
