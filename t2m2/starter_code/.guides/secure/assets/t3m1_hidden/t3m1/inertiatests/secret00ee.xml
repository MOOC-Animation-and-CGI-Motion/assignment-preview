<scene>
  <simtype type="rigid-body"/>
  <description text="Multiple stationairy rigid bodies with no forces. Intended as a 'null' test; nothing should happen."/>
  <maxsimfreq max="10000000.0"/>  
  <duration time="5.0"/>

  <rigidbodyintegrator type="explicit-euler" dt="0.01"/>

  <!-- Rigid Body One -->
  <rigidbodyvertex x="0" y="0" m="6"/>
  <rigidbodyvertex x="2" y="0" m="5"/>
  <rigidbodyvertex x="2" y="3" m="4"/>
  <rigidbodyvertex x="5" y="3" m="3"/>
  <rigidbodyvertex x="5" y="5" m="2"/>
  <rigidbodyvertex x="0" y="5" m="1"/>

  <rigidbody p="0" p="1" p="2" p="3" p="4" p="5" vx="0.0" vy="0.0" omega="0.0" r="0.1"/>

  <!-- Rigid Body Two -->
  <rigidbodyvertex x="8"  y="0" m="8"/>
  <rigidbodyvertex x="10" y="0" m="6"/>
  <rigidbodyvertex x="10" y="3" m="7"/>
  <rigidbodyvertex x="13" y="3" m="5"/>
  <rigidbodyvertex x="13" y="5" m="3"/>
  <rigidbodyvertex x="8"  y="5" m="9"/>

  <rigidbody p="11" p="10" p="9" p="8" p="7" p="6" vx="0.0" vy="0.0" omega="0.0" r="0.4"/>

  <!-- Rigid Body Three -->
  <rigidbodyvertex x="3" y="-1" m="5"/>
  <rigidbodyvertex x="3" y="1"  m="5"/>
  <rigidbodyvertex x="5" y="1"  m="5"/>
  <rigidbodyvertex x="5" y="-1" m="5"/>

  <rigidbody p="12" p="13" p="14" p="15" vx="0.0" vy="0.0" omega="0.0" r="0.02"/>

</scene>
