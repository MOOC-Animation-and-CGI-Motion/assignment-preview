<scene>
  <description text="Multiple springs of the same material parameter (alpha) and different resting length. Observe how the mass oscillates with different periods."/>

  <duration time="20.0"/>
  <integrator type="symplectic-euler" dt="0.01"/>
  <maxsimfreq max="500.0"/>
  <viewport cx="0.0" cy="1.2" size="1.2"/>
  
  <simplegravity fx="0" fy="-0.3" />
  
  <particle m="1.0" px="-0.6" py="2.0" vx="0.0" vy="0.0" fixed="1"/>
  <particle m="1.0" px="-0.6" py="1.75" vx="0.0" vy="0.0" fixed="0"/>
  <edge i="0" j="1"/>
  <elasticbodyspringforce i1="0" i2="1" alpha="1.0" l0="0.25" />

  <particle m="1.0" px="-0.2" py="2.0" vx="0.0" vy="0.0" fixed="1"/>
  <particle m="1.0" px="-0.2" py="1.5" vx="0.0" vy="0.0" fixed="0"/>
  <edge i="2" j="3"/>
  <elasticbodyspringforce i1="2" i2="3" alpha="1.0" l0="0.5" />

  <particle m="1.0" px=" 0.2" py="2.0" vx="0.0" vy="0.0" fixed="1"/>
  <particle m="1.0" px=" 0.2" py="1.25" vx="0.0" vy="0.0" fixed="0"/>
  <edge i="4" j="5"/>
  <elasticbodyspringforce i1="4" i2="5" alpha="1.0" l0="0.75" />

  <particle m="1.0" px=" 0.6" py="2.0" vx="0.0" vy="0.0" fixed="1"/>
  <particle m="1.0" px=" 0.6" py="1.0" vx="0.0" vy="0.0" fixed="0"/>
  <edge i="6" j="7"/>
  <elasticbodyspringforce i1="6" i2="7" alpha="1.0" l0="1.0" />

</scene>
