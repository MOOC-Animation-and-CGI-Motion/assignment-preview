<scene>
  <description text="Same as test08, except with non-zero poisson ratio. This demonstrates the definition of poisson ratio."/>

  <duration time="50.0"/>
  <integrator type="symplectic-euler" dt="0.01"/>
  <maxsimfreq max="500.0"/>

  <particle m="1.0" px="0.0" py="0.0" vx="0.0" vy="0.0" fixed="0"/>
  <particle m="1.0" px="0.8" py="0.0" vx="0.0" vy="0.0" fixed="0"/>
  <particle m="1.0" px="0.0" py="1.0" vx="0.0" vy="0.0" fixed="0"/>
  <particle m="1.0" px="0.8" py="1.0" vx="0.0" vy="0.0" fixed="0"/>

  <edge i="0" j="1"/>
  <edge i="1" j="2"/>
  <edge i="2" j="0"/>
  <edge i="2" j="3"/>
  <edge i="3" j="1"/>
  <elasticbodycstforce i1="0" i2="1" i3="2" youngsmodulus="1.0" poissonratio="0.5" xb3y="0.8"/>
  <elasticbodycstforce i1="3" i2="2" i3="1" youngsmodulus="1.0" poissonratio="0.5" xb1y="0.8" xb2y="0.8"/>
  
</scene>
